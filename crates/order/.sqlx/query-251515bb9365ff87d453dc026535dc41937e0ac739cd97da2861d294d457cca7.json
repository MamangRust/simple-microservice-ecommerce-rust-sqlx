{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            order_item_id,\n            order_id,\n            product_id,\n            quantity,\n            price,\n            created_at,\n            updated_at,\n            deleted_at,\n            COUNT(*) OVER() AS total_count\n        FROM order_items\n        WHERE ($1::TEXT IS NULL OR product_id::TEXT ILIKE '%' || $1 || '%')\n        ORDER BY created_at DESC\n        LIMIT $2 OFFSET $3\n        ",
